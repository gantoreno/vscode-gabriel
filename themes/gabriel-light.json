  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#c0c0c0"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#999dd4"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.language",
        "entity"
      ],
      "settings": {
        "foreground": "#999dd4"
      }
    },
    {
      "scope": [
        "entity.name",
        "meta.export.default",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#7ba2c3"
      }
    },
    {
      "scope": [
        "variable.parameter.function",
        "meta.jsx.children",
        "meta.block",
        "meta.tag.attributes",
        "entity.name.constant",
        "meta.object.member",
        "meta.embedded.expression"
      ],
      "settings": {
        "foreground": "#101010"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#a2a6d4"
      }
    },
    {
      "scope": ["entity.name.tag", "support.class.component"],
      "settings": {
        "foreground": "#7ba2c3"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#ac616a"
      }
    },
    {
      "scope": ["keyword.operator", "storage.type.function.arrow"],
      "settings": {
        "foreground": "#ac616a"
      }
    },
    {
      "scope": ["storage", "storage.type"],
      "settings": {
        "foreground": "#ac616a"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#101010"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#7ba2c3"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#7ba2c3"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#7ba2c3"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#101010"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#101010"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "foreground": "#b3646e"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#b3646e"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#b3646e"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "foreground": "#b3646e"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "foreground": "#f0f0f0"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#b3646e"
      }
    },
    {
      "scope": "string source",
      "settings": {
        "foreground": "#101010"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#7ba2c3"
      }
    },
    {
      "scope": ["source.regexp", "string.regexp"],
      "settings": {
        "foreground": "#7ba2c3"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#7ba2c3"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "foreground": "#7ba2c3",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#7ba2c3"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#7ba2c3"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#7ba2c3"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#bfa86b"
      }
    },
    {
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "foreground": "#7ba2c3",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#7ba2c3"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#101010",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#101010",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.raw",
      "settings": {
        "foreground": "#7ba2c3"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "foreground": "#b3646e"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "foreground": "#7ba2c3"
      }
    },
    {
      "scope": ["markup.changed", "punctuation.definition.changed"],
      "settings": {
        "foreground": "#bfa86b"
      }
    },
    {
      "scope": ["markup.ignored", "markup.untracked"],
      "settings": {
        "foreground": "#2d2d2d"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#a2a6d4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#7ba2c3"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "foreground": "#7ba2c3",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#7ba2c3"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#101010"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#b3646e"
      }
    },
    {
      "scope": ["constant.other.reference.link", "string.other.link"],
      "settings": {
        "foreground": "#7ba2c3",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#bfa86b"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#b3646e"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#a2a6d4"
      }
    }
  ]
}
